      "MonoBehavior_3170": {
        "file": "Clickable.js",
        "inventoryItem": {
          "active": false,
          "author": "Technocrat",
          "autoUpdate": false,
          "created": 1758844109618,
          "data": "this.default = {\n    clickEventName: {\n        \"type\": \"string\",\n        \"value\": \"button-clicked\"\n    },\n    tag: {\n        \"type\": \"string\",\n        \"value\": \"play\"\n    }\n}\n\nObject.entries(this.default).forEach(([key, val])=>{\n    if(!this.vars[key]){\n        this.vars[key] = val\n    }\n})    \n\nthis.onStart = ()=>{\n    this._entity._bs.On(\"click\", e => {\n        log(\"clickable\", \"click\")\n        const event = new CustomEvent(this.vars.clickEventName.value, {\n            detail: {\n                entity: this._entity,\n                ctx: this,\n                details: e.detail,\n                flag: this.vars.tag.value\n            },\n            \n        });\n        window.dispatchEvent(event);\n    })\n}\n\nthis.onDestroy = ()=>{\n    this._entity._bs.listeners.get(\"click\").clear();\n}",
